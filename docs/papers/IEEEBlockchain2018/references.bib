  isbn      = {978-1-4503-4662-7},
  keywords  = {Bitcoin, Blockchain, Distributed hash table, Sharding},
  location  = {Trento, Italy},
  numpages  = {6},
  owner     = {mwalker},
  timestamp = {2017-04-19},
  url       = {http://doi.acm.org/10.1145/3008167.3008170},
}

